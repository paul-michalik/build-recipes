        <BuildMacro Include="DevkitCommonTargetFullPath">
            <Value>$(DevkitCommonTargetFullPath)</Value>
        </BuildMacro>
        <BuildMacro Include="DevkitCommonTargetMiddlePath">
            <Value>$(DevkitCommonTargetMiddlePath)</Value>
        </BuildMacro>
        <BuildMacro Include="DevkitCommonTargetShortPath">
            <Value>$(DevkitCommonTargetShortPath)</Value>
        </BuildMacro>
        <BuildMacro Include="DevkitCommonTargetFullName">
            <Value>$(DevkitCommonTargetFullName)</Value>
        </BuildMacro>
        <BuildMacro Include="DevkitCommonTargetMiddleName">
            <Value>$(DevkitCommonTargetMiddleName)</Value>
        </BuildMacro>
        <BuildMacro Include="DevkitCommonTargetShortName">
            <Value>$(DevkitCommonTargetShortName)</Value>
        </BuildMacro>
    </ItemGroup>
</Project>