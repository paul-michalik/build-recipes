<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup Label="UserMacros">
        <DevkitCommonTargetFullPath>$(PlatformShortName)\$(PlatformToolset)\$(Configuration)</DevkitCommonTargetFullPath>
        <DevkitCommonTargetMiddlePath>$(PlatformShortName)\$(PlatformToolset)</DevkitCommonTargetMiddlePath>
        <DevkitCommonTargetShortPath>$(PlatformShortName)</DevkitCommonTargetShortPath>
        <DevkitCommonTargetFullName>$(ProjectName).$(PlatformShortName).$(PlatformToolset).$(Configuration)</DevkitCommonTargetFullName>
        <DevkitCommonTargetMiddleName>$(ProjectName).$(PlatformShortName).$(PlatformToolset)</DevkitCommonTargetMiddleName>
        <DevkitCommonTargetShortName>$(ProjectName).$(PlatformShortName)</DevkitCommonTargetShortName>
    </PropertyGroup>
    <PropertyGroup />
