<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Label="UserMacros">
    <DevkitCommonBuildTool>msbuild</DevkitCommonBuildTool>
    <DevkitCommonRootDir>$(SolutionDir)..\..</DevkitCommonRootDir>
    <DevkitCommonRootTargetDir>$(DevkitCommonRootDir)\products\msbuild.$(ApplicationType).$(DevkitCommonTargetFullName)</DevkitCommonRootTargetDir>
    <DevkitCommonExtDir>$(Userprofile)\Software</DevkitCommonExtDir>
    <ApplicationType>Windows</ApplicationType>
  </PropertyGroup>
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="DevkitCommonRootDir">
      <Value>$(DevkitCommonRootDir)</Value>
    </BuildMacro>
    <BuildMacro Include="DevkitCommonRootTargetDir">
      <Value>$(DevkitCommonRootTargetDir)</Value>
    </BuildMacro>
    <BuildMacro Include="DevkitCommonExtDir">
      <Value>$(DevkitCommonExtDir)</Value>
    </BuildMacro>
    <BuildMacro Include="ApplicationType">
      <Value>$(ApplicationType)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>